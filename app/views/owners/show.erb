<h1> <%=@owner.username %> </h1>

<h3> Name: <%=@owner.name %> </h3>

<h3> Dog(s): </h3>
<ul>
    <% @owner.dogs.each do |dog| %>
        <li> <%= link_to dog.name, dog_path(dog) %></li>
    <%end%>
</ul>

<h3>Walks: </h3>
<ul>
    <% @owner.get_walks.each do |walk| %>
        <li> <%= link_to walk, walk_path(walk) %></li>
    <%end%>
</ul>